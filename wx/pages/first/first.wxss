/**first.wxss**/
.first-content{
  padding-top: 120rpx;
  border-top:  1rpx solid #D9D9D9;
  
}

.show{
  display:block;
  text-align:center;
  line-height:200rpx;
  font-size: 18px;
}

.hidden{
  display:none;
  text-align:center;
  line-height:200rpx;
}

.default{
  text-align:center;
  flex:1;
  border-right:1px solid gainsboro;
  color:#000;
  font-weight:bold;
  font-size:38rpx;
}

.blue{
  text-align:center;
  color:#79CDCD;
  flex:1;
  border-right:1px solid gainsboro;
  font-weight:bold;
  font-size:38rpx;
}

.list{
  width:100%;
  height:70rpx;
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  margin-top: 30rpx;
}

.first-cell-rect:active{
  background-color: #ECECEC;
}

.mid-line{
  border-bottom: 1rpx solid #D9D9D9;
}
